{
    "name": "cleannote",
    "description": "a minimal note-taking extension with sync.",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl + Shift + L"
            }
        }
    },
    "icons": {
        "128": "icons/512x512.png"
    }
}

